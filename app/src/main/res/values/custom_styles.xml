<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Circular">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>

    <style name="CircularIcon">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">5dp</item>
    </style>

    <style name="rectangle">
        <item name="cardCornerRadius">5dp</item>
    </style>
    <style name="ProfileEditText">
        <item name="android:paddingTop">5dp</item>
        <item name="paddingStart">50dp</item>
        <item name="paddingEnd">50dp</item>
        <item name="cardCornerRadius">5dp</item>
        <item name="color">@color/black</item>
    </style>

    <style name="PageHeadingStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">20sp</item>
        <item name="android:fontFamily">@font/fredoka_one</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="list_style">
        <item name="android:textAlignment">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/david_libre_medium</item>
        <item name="android:textSize">18sp</item>
    </style>
</resources>